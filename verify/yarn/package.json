{
  "name": "mlc-verify-yarn",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@typefox/monaco-editor-react": "~3.1.0",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~4.1.2",
    "monaco-editor-wrapper": "~4.1.0",
    "monaco-languageclient-examples": "~8.2.0",
    "vscode": "npm:@codingame/monaco-vscode-api@~4.1.2",
    "vscode-ws-jsonrpc": "~3.3.1"
  },
  "devDependencies": {
    "http-server": "~14.1.1",
    "shx": "~0.3.4",
    "vite": "~5.1.6"
  },
  "volta": {
    "node": "20.12.0",
    "yarn": "1.22.22"
  },
  "scripts": {
    "verify": "yarn install && yarn run build && yarn run start",
    "verify:ci": "yarn install && yarn run build",
    "clean": "shx rm -fr dist",
    "build:msg": "echo Building yarn/vite example:",
    "build": "yarn run build:msg && yarn run clean && vite build",
    "start": "http-server ./dist --port 8084"
  }
}
