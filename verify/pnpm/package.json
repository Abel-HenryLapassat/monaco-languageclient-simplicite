{
  "name": "mlc-verify-pnpm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.19.0",
    "pnpm": ">=9.15.0"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "9.15.4"
  },
  "dependencies": {
    "monaco-languageclient-examples": "~2025.2.4"
  },
  "devDependencies": {
    "shx": "~0.3.4",
    "vite": "~6.0.11"
  },
  "scripts": {
    "verify": "pnpm install && pnpm run build && pnpm run start",
    "verify:ci": "pnpm install && pnpm run build",
    "clean": "shx rm -fr dist",
    "build:msg": "echo Building pnpm/vite example:",
    "build": "pnpm run build:msg && pnpm run clean && vite build",
    "start": "vite preview"
  }
}
