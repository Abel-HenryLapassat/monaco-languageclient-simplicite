{
  "name": "example-client-vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~4.3.2",
    "monaco-editor-wrapper": "../../packages/wrapper",
    "monaco-languageclient-examples": "../../packages/examples",
    "vscode": "npm:@codingame/monaco-vscode-api@~4.3.2"
  },
  "devDependencies": {
    "http-server": "~14.1.1",
    "shx": "~0.3.4",
    "vite": "~5.2.8"
  },
  "volta": {
    "node": "20.12.2",
