{
  "name": "example-client-vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "monaco-editor-workers": "0.34.2",
    "monaco-languageclient": "5.0.0-next.0",
    "vscode-languageclient": "~8.0.2",
    "vscode-ws-jsonrpc": "3.0.0-next.0",
    "normalize-url": "~8.0.0"
  },
  "devDependencies": {
    "http-server": "~14.1.1"
  },
  "volta": {
    "node": "18.14.0",
    "npm": "9.4.1"
  },
  "scripts": {
    "clean": "npx shx rm -fr dist src",
    "vite:prepare": "npx shx mkdir -p ./src/client && npx shx cp -f ../../examples/main/src/client/main.ts ./src/client/main.ts",
    "copy:monacoworkers": "shx mkdir -p dist/dist && shx cp -r ../../../node_modules/monaco-editor-workers/dist/workers/editorWorker* ./dist/dist",
    "build:msg": "echo Building client-vite example:",
    "build": "npm run build:msg && npm run clean && npm run vite:prepare && vite build && npm run copy:monacoworkers",
    "start": "http-server --port 8082 ./dist"
  }
}
