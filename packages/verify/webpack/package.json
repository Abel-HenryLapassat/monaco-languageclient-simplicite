{
  "name": "example-client-webpack",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "monaco-editor-workers": "0.34.2",
    "monaco-languageclient": "5.0.0-next.0",
    "vscode-languageclient": "~8.0.2",
    "vscode-ws-jsonrpc": "3.0.0-next.0",
    "normalize-url": "~8.0.0"
  },
  "devDependencies": {
    "css-loader": "~6.7.3",
    "path-browserify": "~1.0.1",
    "source-map-loader": "~4.0.1",
    "style-loader": "~3.3.1",
    "ts-loader": "~9.4.2",
    "webpack-cli": "~5.0.1",
    "http-server": "~14.1.1"
  },
  "volta": {
    "node": "18.14.0",
    "npm": "9.4.1"
  },
  "scripts": {
    "clean": "npx shx rm -fr dist src *.tsbuildinfo",
    "webpack:prepare": "npx shx mkdir -p ./src/client && npx shx cp -f ../../examples/main/src/client/main.ts ./src/client/main.ts",
    "copy:monacoworkers": "shx mkdir -p dist && shx cp -r ../../../node_modules/monaco-editor-workers/dist/workers/editorWorker* ./dist",
    "build:msg": "echo Building client-webpack example:",
    "build": "npm run build:msg && npm run clean && npm run webpack:prepare && webpack && npm run copy:monacoworkers",
    "start": "http-server --port 8081 ./"
  }
}
