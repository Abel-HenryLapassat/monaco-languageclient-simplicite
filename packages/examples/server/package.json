{
  "name": "server-example",
  "private": true,
  "type": "module",
  "dependencies": {
    "vscode-ws-jsonrpc": "../../vscode-ws-jsonrpc",
    "@types/ws": "^8.5.3",
    "express": "^4.18.1",
    "normalize-url": "^7.0.3",
    "request-light": "^0.5.8",
    "vscode-json-languageservice": "5.1.0",
    "vscode-languageserver": "8.0.2",
