{
  "name": "monaco-editor-wrapper",
  "version": "6.2.2",
  "license": "MIT",
  "description": "Wrapper for monaco-vscode-editor-api and monaco-languageclient",
  "keywords": [
    "monaco-editor",
    "monaco-languageclient",
    "typescript",
    "vscode"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vscode/services": {
      "types": "./dist/vscode/index.d.ts",
      "default": "./dist/vscode/index.js"
    },
    "./vscode/locale": {
      "types": "./dist/vscode/localeLoader.d.ts",
      "default": "./dist/vscode/localeLoader.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index"
      ],
      "vscode/services": [
        "dist/vscode/services"
      ],
      "vscode/locale": [
        "dist/vscode/localeLoader"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "vite-node ../../scripts/clean.ts --relativePath packages/wrapper --recursive --paths ./dist ./bundle *.tsbuildinfo",
    "compile": "tsc --build tsconfig.src.json && tsc --build tsconfig.test.json",
    "build": "npm run clean && npm run compile"
  },
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=10.2.3"
  },
  "volta": {
    "node": "22.13.1",
    "npm": "10.9.2"
  },
  "dependencies": {
    "@codingame/monaco-vscode-api": "13.1.2",
    "@codingame/monaco-vscode-configuration-service-override": "13.1.2",
    "@codingame/monaco-vscode-editor-api": "13.1.2",
    "@codingame/monaco-vscode-editor-service-override": "13.1.2",
    "@codingame/monaco-vscode-extension-api": "13.1.2",
    "@codingame/monaco-vscode-language-pack-cs": "13.1.2",
    "@codingame/monaco-vscode-language-pack-de": "13.1.2",
    "@codingame/monaco-vscode-language-pack-es": "13.1.2",
    "@codingame/monaco-vscode-language-pack-fr": "13.1.2",
    "@codingame/monaco-vscode-language-pack-it": "13.1.2",
    "@codingame/monaco-vscode-language-pack-ja": "13.1.2",
    "@codingame/monaco-vscode-language-pack-ko": "13.1.2",
    "@codingame/monaco-vscode-language-pack-pl": "13.1.2",
    "@codingame/monaco-vscode-language-pack-pt-br": "13.1.2",
    "@codingame/monaco-vscode-language-pack-qps-ploc": "13.1.2",
    "@codingame/monaco-vscode-language-pack-ru": "13.1.2",
    "@codingame/monaco-vscode-language-pack-tr": "13.1.2",
    "@codingame/monaco-vscode-language-pack-zh-hans": "13.1.2",
    "@codingame/monaco-vscode-language-pack-zh-hant": "13.1.2",
    "@codingame/monaco-vscode-monarch-service-override": "13.1.2",
    "@codingame/monaco-vscode-textmate-service-override": "13.1.2",
    "@codingame/monaco-vscode-theme-defaults-default-extension": "13.1.2",
    "@codingame/monaco-vscode-theme-service-override": "13.1.2",
    "@codingame/monaco-vscode-views-service-override": "13.1.2",
    "@codingame/monaco-vscode-workbench-service-override": "13.1.2",
    "monaco-languageclient": "~9.2.2",
    "vscode": "npm:@codingame/monaco-vscode-extension-api@13.1.2",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver-protocol": "~3.17.5",
    "vscode-ws-jsonrpc": "~3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TypeFox/monaco-languageclient.git",
    "directory": "packages/wrapper"
  },
  "homepage": "https://github.com/TypeFox/monaco-languageclient/blob/main/packages/wrapper/README.md",
  "bugs": "https://github.com/TypeFox/monaco-languageclient/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
