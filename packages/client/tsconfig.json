{
  "extends": "../../tsconfig",
  "compilerOptions": {
      "rootDir": "src",
      "outDir": "lib",
      "declarationDir": "lib",
      "target": "es2020",
      "module": "es2020",
      "moduleResolution": "node",
      "lib": [
        "es2016",
        "dom"
      ],
      "noUnusedParameters": false
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
    "lib",
    "node_modules"
  ]
}
